{
  "name": "lending-app",
  "author": "ahihi",
  "licence": "MIT",
  "version": "1.0.0",
  "private": false,
  "homepage": "http://lending-app.dev/",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "npx eslint \"src/**/*.@(tsx|ts|jsx|js)\" --fix ; npx prettier --check \"src/**/*.@(tsx|ts|jsx|js)\" --write"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^36.0.1",
    "@ckeditor/ckeditor5-react": "^5.1.0",
    "@ckeditor/ckeditor5-upload": "^36.0.1",
    "@craco/craco": "^6.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.28",
    "@material-ui/icons": "^5.0.0-alpha.28",
    "@material-ui/styles": "^4.10.0",
    "@mui-treasury/components": "^1.10.1",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/styles": "^5.15.3",
    "@mui/x-date-pickers": "^6.0.4",
    "@reduxjs/toolkit": "^1.5.1",
    "@tinymce/tinymce-react": "^4.3.0",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "clsx": "^1.1.1",
    "faker": "5.5.3",
    "file-saver": "^2.0.5",
    "formik": "^2.1.5",
    "history": "^5.0.0",
    "install": "^0.13.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "mui-datatables": "^4.3.0",
    "npm": "^7.7.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^1.2.5",
    "react-feather": "^2.0.8",
    "react-helmet": "^6.1.0",
    "react-intl-hooks": "^1.0.11",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^4.0.0",
    "react-toastify": "9.0.0",
    "uuid": "^8.3.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@mui/material": "^5.11.12",
    "autoprefixer": "^9.8.8",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-local-rules": "^0.1.1",
    "eslint-plugin-prefer-arrow": "^1.1.6",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "postcss": "^7.0.39",
    "postcss-cli": "^11.0.0",
    "prettier": "^1.19.1",
    "react-highcharts": "^16.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
  }
}
